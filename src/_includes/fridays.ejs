<div class="events-list">
  <% collections.studyEvents.forEach(event => { %>

  <div class="event">
    <div class="heading-line">
      <div class="event-time">
        <%= formatDate(toEst(event.data.date), "EEEE, MMMM do 'from' h") %>-<%=
        formatDate(toEst(event.data.endDate), 'h bbbb zzz' ) %>
      </div>
      <% if (event.data.cohortOnly) { %>
      <div class="cohort-only">(Cohort-Only)</div>
      <% } %> <% if (event.data.rsvpUrl) { %>
      <a class="rsvp-link" href="<%= event.data.rsvpUrl %>" target="_blank"
        >RSVP</a
      >
      <% } %>
    </div>

    <h3 class="event-title"><%= event.data.title %></h3>
    <div class="event-body"><%- event.templateContent %></div>
  </div>
  <% }) %>
</div>
